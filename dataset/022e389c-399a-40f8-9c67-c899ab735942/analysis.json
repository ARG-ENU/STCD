    },
    {
      "id": "ef499cef-6cf9-4ef7-91b5-32f002da049e",
      "type": "atom",
      "text": "Walking at 3mph for 30 minutes, a 60kg person will burn 99 calories",
      "sources": [
        {
          "resource_id": "cf0593a7-687a-4413-94b5-f14e1588429a",
          "text": "Walking at 3mph for 30 minutes, a 60kg person will burn 99 calories",
          "offset": 0,
          "length": 0
        }
      ],
      "metadata": {}
    },
    {
      "id": "9705ec05-a130-4fcf-a248-d28e7a350578",
      "type": "atom",
      "text": "Walking the NHS advised 10,000 steps a day will significantly improve your health",
      "sources": [
        {
          "resource_id": "cf0593a7-687a-4413-94b5-f14e1588429a",
          "text": "Walking the NHS advised 10,000 steps a day will significantly improve your health",
          "offset": 0,
          "length": 0
        }
      ],
      "metadata": {}
    },
    {
